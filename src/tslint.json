{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "import-blacklist": [
          true,
          "rxjs/Rx",
          "ngx-api-utils/public_api",
          "packages/ngx-api-utils/src/public_api"
        ]
    }
}
