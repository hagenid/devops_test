{
    "extends": "../../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "lib",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "lib",
            "kebab-case"
        ],
        "import-blacklist": [
          true,
          "rxjs/Rx",
          "ngx-api-utils",
          "ngx-api-utils/public-api"
        ]
    }
}
